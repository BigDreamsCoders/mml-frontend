{
    "name": "proyecto",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "axios": "^0.19.0",
        "bootstrap": "^4.3.1",
        "node-sass": "^7.0.0",
        "pace": "0.0.4",
        "pace-js": "^1.0.2",
        "prop-types": "^15.7.2",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "react-helmet": "^5.2.1",
        "react-rater": "^5.1.1",
        "react-router-dom": "^5.1.1",
        "react-scripts": "3.1.2",
        "react-tabs": "^3.0.0",
        "reactstrap": "^8.0.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint:fix": "eslint src/*.js",
        "lint": "eslint src/*.js",
        "apply-format": "prettier --write \"**/*.*\"",
        "check-format": "prettier --check \"**/*.*\""
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "husky": "^3.0.9",
        "lint-staged": "^9.4.2",
        "prettier": "^1.18.2",
        "react-test-renderer": "^16.10.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-commit-msg": ".git/hooks/my-preparecommit-msg.sample",
            "pre-push": "npm run check-format"
        }
    },
    "lint-staged": {
        "*.js": [
            "prettier --write",
            "git add"
        ]
    }
}
